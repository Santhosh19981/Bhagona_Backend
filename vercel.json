{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/uploads/(.*)",
            "dest": "/uploads/$1"
        },
        {
            "src": "/(.*)",
            "dest": "src/index.js"
        }
    ],
    "env": {
        "DB_HOST": "@db_host",
        "DB_PORT": "@db_port",
        "DB_USER": "@db_user",
        "DB_PASSWORD": "@db_password",
        "DB_NAME": "@db_name"
    }
}